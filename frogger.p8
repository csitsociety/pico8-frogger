function _update()
 -- if camera isn't moving
 if not cam_animate then
		update_frog()
		update_cars()
		update_smogs()
	end
	
	-- restart when dead
	if dead and btnp(üÖæÔ∏è) then
	 if not has_control then
	  frog.y = 62
	 else
			frog.y = flr(frog.y/16)*16 + 15
	 end
	 has_control = true
	 dead = false
		frog.x = 7
	end
end

function _draw()
 cls()
 set_cam()
 
 -- draw background
	map()
	
	-- draw game elements
	draw_smogs()
	draw_sun()
	draw_splat()
	draw_cars()
	draw_frog()
end


-- ‚ñí spawning ‚ñí

-- kind=0 -> car
-- kind=1 -> van
-- kind=2 -> truck head
-- kind=3 -> truck carriage
-- kind=4 -> log start
-- kind=5 -> log middle
-- kind=6 -> lod end
function add_car(x,y,kind,dir)
 return add(cars, {
 	x = x,
 	y = y,
 	kind = kind,
 	dir = dir, -- either -1 or 1
 	col = rnd() > .5 and 1 or 0,
 	is_log = kind > 3,
 })
end

-- helper to add truck
function add_truck(x, y, len, dir)
	local head = add_car(x, y, 2, dir)
	for i=0,len-2 do
	 local xo = dir == 1 and -8 or 0
		local t = add_car(x-17*i*dir-(dir*8)+xo, y, 3, dir)
		t.col = head.col
	end
end

function add_log(x, y, len, dir)
	add_car(x, y, 4, dir)
	for i=1, len-2 do
		local xo = dir == 1 and 0 or 0
		add_car(x+8*i, y, 5, dir)
	end
	add_car(x+len*8 - 8, y, 6, dir)
end

function add_smog(x,y)
	return add(smogs, {
		x = x,
		y = y,
		r = flr(rnd()*2)
	})
end

-- ‚ñí processing ‚ñí

function update_frog()
 -- determine input
 if has_control and not dead then
	 local dx = 0
	 local dy = 0
	 local did_move = false
		if btnp(‚û°Ô∏è) then
			dx = 1
			frog.dir = 1
			did_move = true
		end
		if btnp(‚¨ÖÔ∏è) then
			dx = -1
			frog.dir = 3
			did_move = true
		end
		if btnp(‚¨áÔ∏è) then
			dy = 1
			frog.dir = 2
			did_move = true
		end
		if btnp(‚¨ÜÔ∏è) then
			dy = -1
			frog.dir = 0
			did_move = true
		end
		
		-- reset log offset
		if did_move then
		 frog.x += flr(frog.xo/8)
		 frog.xo = frog.xo % 8
		 local ft = mget(frog.x, frog.y)
		 if not (ft == 19 or ft == 35) then
		 	frog.xo = 0
		 end
		end
		
		-- apply movement
		frog.x = clamp(frog.x + dx, 0, 15)
		frog.y = clamp(frog.y + dy, 0, last_screen*16+15)
	end

 -- off screen?
 local real_x = frog.x*8 + frog.xo
 if real_x < 0 or real_x > 128 then
 	dead = true
 end
 
 -- special secret thing
 if frog.y == 13 and has_control then
 	has_control = false
 	local c = add_car(-8, 13*8, 1, 1)
  c.col = 1
 end
 
 -- get hit?
 for c in all(cars) do
 	if not c.is_log then
 		if car_ints_frog(c) then
 			dead = true
 		end
 	end
 end
 
 -- on water?
 local mtile = mget(frog.x, frog.y)
 if mtile == 19 or mtile == 35 then
  local on_log = false
  for c in all(cars) do
  	if c.is_log then
  		if car_ints_frog(c) then
  			on_log = true
  		end
  	end
  end
  if not on_log then
  	dead = true
  end
 end
end


function update_cars()
 for c in all(cars) do
  -- move along
  c.x += 1 * c.dir
 	
 	-- check if off screen
 	local off_r = c.dir == 1 and
 															c.x >= 136
 	local off_l = c.dir == -1 and
 															c.x <= -16
  
  -- if off screen, respawn
  if off_r or off_l then
  	del(cars, c)
  	local nc = add_car(c.x, c.y, c.kind, c.dir)
  	nc.x = nc.dir == 1 and -16 or 136
  	nc.col = c.col 
  end
  
  -- spawn smoke smogs
  if c.kind == 1 then
	  add_smog(c.x + (c.dir == -1 and 16 or 0), c.y+4)
  end
 end
end

function update_smogs()
	for c in all(smogs) do
		c.y -= .3
		if rnd() < .13 then
			del(smogs, c)
		end
	end
end

function car_ints_frog(c)
	if c.y == frog.y*8 then
   local is_int = false
  	local c1 = c.x
  	local c2 = c.x + (c.is_log and 8 or 16)
  	if not c.is_log then
  	 c2 -= 3
				c1 += 3
  	else
  		c1 -= 2
  	end
  	local f1 = frog.x*8 + frog.xo
  	local f2 = frog.x*8 + 8 + frog.xo
  	return (f1 > c1 and f1 < c2) or
  		      (f2 > c1 and f2 < c2)
 end
 return false
end


-- ‚ñí rendering ‚ñí

function set_cam()
 -- determine camera position
 local screen = clamp(flr((frog.y)/16), 0, last_screen)
 local des_cam_y = screen*16*8
 
 -- transition camera position
 if cam_y == nil then
 	cam_y = des_cam_y
 else
  if cam_y != des_cam_y then
   cam_animate = true
 	 cam_y = cam_y + 4*sgn(des_cam_y-cam_y)
 	else
 		cam_animate = false
 	end
 end
 
 -- set camera offset
 camera(0, cam_y)
end

function draw_smogs()
	for c in all(smogs) do
		circfill(c.x, c.y, c.r, 13)
	end
end

function draw_splat()
 if dead then
  local mt = mget(frog.x, frog.y)
  if mt != 19 and mt != 35 then
		 spr(56, frog.x*8, frog.y*8)
		end
	end
end

function draw_cars()
 for c in all(cars) do
  -- car rendering fx
  if not c.is_log then
	  -- shadow
	  pal(11, 1)
	  
	  -- colour variation
	  if c.col == 1 then
	  	pal(8, 11)
	  	pal(2, 3)
	  	pal(14, 10)
	  end
	 
	  -- wheel spin fx
	  if sin(time()*10) < 0 then
	  	pal(1, 5)
	  	pal(5, 1)
	  end
  end
 
 	-- car or van
 	if c.kind <= 1 then
 	 spr(16+c.kind*16, c.x, c.y-2, 2, 1, c.dir == -1)
 	end

  -- truck head
	 if c.kind == 2 then
	  spr(50, c.x, c.y-2, 1, 1, c.dir == -1)
		end
		
		-- truck carriage
		if c.kind == 3 then
		 local xo = c.dir == 1 and 16 or 0
		 line(c.x+xo, c.y+3, c.x+xo+c.dir/2, c.y+3, 11)
			spr(48, c.x, c.y-2, 2, 1, c.dir == -1)
		end
		
		if c.kind >= 4 and c.kind <= 6 then
			spr(36+c.kind-4, c.x, c.y-1, 1, 1)
		end
	 
	 pal()
 end
end


function draw_frog()
	if dead then
		return
	end

	local fx = frog.x*8
	local fy = frog.y*8
	
	-- when in river, move up
	local tc = mget(frog.x, frog.y)
	if tc == 35 or tc == 19 then
		fy -= 2
		
		for c in all(cars) do
			if c.is_log and c.y == frog.y*8 then
			 fx += frog.xo
		  frog.xo += c.dir
		  break
			end
		end		
	end
	
	-- determine how to draw
	local horizontal = frog.dir % 2 == 1
	local sprite = horizontal and 21 or 20
	local do_flip = frog.dir > 1
	
	if dead then
		sprite = 56
	end
	
	-- draw shadow
	pal_all(1)
	spr(sprite, fx, fy+1, 1, 1, do_flip, do_flip)
	pal()
	
	-- draw frog
 spr(sprite, fx, fy, 1, 1, do_flip, do_flip)
 pal()
end


function draw_sun()
	if frog.y < 16 then
		circfill(59, 43, 15, 9)
		for xx=30,75 do
			for yy=44,64 do
				if pget(xx,yy) == 9 then
					pset(xx, yy, 10)
				end
			end
		end
		
		for i=0, 4 do
			local xx = 10 + (128 / 5) * i
			local yy = 20+sin(xx/10)*8
			for j=1,4 do
			 local dx=cos((1/j)+i/4)*8
			 local dy=sin((1/j)+i/5)*5
			 --if j %2 == 0 then fillp(‚ñ•) else fillp() end 
			 circfill(((xx+dx)+flr(time()*8))%128, yy+dy/3 + 1, 4+j/2, 9)
			 circfill(((xx+dx)+flr(time()*8))%128, yy+dy/3, 4+j/2, 13)
			end
		end
		
		print('thx for playing ‚ô•', 10, 10, 7)
	end
end


-- ‚ñí util ‚ñí

function pal_all(c)
	for i=1, 16 do
		pal(i, c)
	end
end

function clamp(x, x_min, x_max)
	return max(min(x, x_max), x_min)
end

function round(x)
 local d = x % 1
 if d > .5 then
 	return ceil(x)
 else
 	return flr(x)
 end
end
